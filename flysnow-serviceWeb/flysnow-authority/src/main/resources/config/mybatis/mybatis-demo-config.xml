<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <!-- 命名空间 -->
    <typeAliases>
        <!-- 扫描model包 使用别名 -->
        <!-- <package name="com.flysnow.demo.model"/> -->
        <!-- 预约图书表 -->
        <typeAlias alias="appointment" type="com.flysnow.demo.model.Appointment"/>
        <!-- 图书表 -->
        <typeAlias alias="book" type="com.flysnow.demo.model.Book"/>
    </typeAliases>

    <!-- 映射mapper-->
    <mappers>
        <!-- 预约图书表 -->
        <mapper resource="config/mybatis/demo/AppointmentMapper.xml"/>
        <!-- 图书表 -->
        <mapper resource="config/mybatis/demo/BookMapper.xml"/>
    </mappers>
</configuration>
